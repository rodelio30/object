<p id="notice"><%= notice %></p>


<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
  <tbody>
    <% @todo_lists.each do |todo_list| %>
    <tr>
    <td><%= link_to todo_list.title, todo_list, class: 'text-white' %></td>
    <td><%= todo_list.description %></td>
  </tr>
  <% end %>
  </tbody>
</table>
<div class="row">
  <div class="col"><%= link_to 'New Todo List', new_todo_list_path, class: 'btn btn-outline-success', style: 'float: right' %></div>
</div>
